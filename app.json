{
	"expo": {
		"name": "Simple Calorie Tracker",
		"slug": "simple-calorie-tracker",
		"version": "1.2.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.antomanc.simplecalorietracker"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/foreground.png",
				"backgroundColor": "#111928"
			},
			"package": "com.antomanc.simplecalorietracker"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/foreground.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#111928"
				}
			],
			[
				"react-native-vision-camera",
				{
					"cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
					"enableCodeScanner": true
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
